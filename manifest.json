{
   "manifest_version": 2,

   "name": "Elasticseach Upload",
   "description": "A utility to upload data into ElasticSearch from CSV file.",
    "version": "0.1.0",

    "icons": {
   },

   "permissions": [
      "https://*/",
      "http://*/",
      "tabs",
      "clipboardWrite"
   ],
   
   "browser_action": {
      "default_title": "Click here!"
   },
    "background": {
      "scripts": ["js/background.js"]
   },
   
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
