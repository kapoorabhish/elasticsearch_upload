{
   "manifest_version": 2,

   "name": "Elasticsearch Upload",
   "description": "A utility to upload data to ElasticSearch from a csv from browser only.",
    "version": "0.1.0",

   "permissions": [
      "https://*/",
      "http://*/",
      "tabs",
      "clipboardWrite"
   ],
   "browser_action": {
      "text":"Elasticsearch Upload"
   },
   "background": {
      "scripts": ["js/background.js"]
   },
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}